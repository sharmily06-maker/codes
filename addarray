package com.java;

public class addarray {
	public static void main(String[]args) {
		int a[]= {231,345,425,324};
		int n=a.length;
		int val[]=new int[n];
		for(int i=0;i<a.length;i++);
		    int sum=0;
		    int i=0;
		    while(a[i]>0) {
		    int dig=a[i]%10;
		    sum=sum+dig;
		    a[i]=a[i]/10;
		    }
		    val[i]=sum;	
          for(int i=0;i<a.length;i++) {
        	  if(val[i]%2==1) {
        		  System.out.println("val[i]");
        	  }
          }
	}
}
