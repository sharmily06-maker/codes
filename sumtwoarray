package com.java;

public class sumtwodarray {
	public static void main(String[]args) {
		int [] []a= {{05,20,40,96},
				     {65,76,87,23},
				     {45,78,90,65},
				     {34,54,23,12}
		             };
		            int total=0;
		   for(int i=0;i<a.length;i++) {
			   for(int j=0;j<a.length;j++) {
				   total=total+a[i][j];
			   }
		   }
                System.out.println(total);
}
}
