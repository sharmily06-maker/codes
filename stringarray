package com.java;

public class stringarray {
	public static void main(String[]args) {
		String [][]a= {{"BMW","BENZ","LAMBOGINI","AUDI"},
				     {"rino","banu","FERARI","PORCHE"},
				     {"sonic","nick","jim","lady"},
				     {"red","bull","mouse","joy"}};
		          int count=1;
		          for(int i=0;i<a.length;i++) {
		        	  for(int j=0;j<a.length;j++) {
		        		  System.out.println(count +";"+a[i][j]);
		        		  count++;
		        	  }
		}
	}

}
